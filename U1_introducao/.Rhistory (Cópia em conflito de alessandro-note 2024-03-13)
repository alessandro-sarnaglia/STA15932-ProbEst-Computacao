rep(10, 8)
t1
n1 <- rep(10, 8)
n2 <- 10^(1:8)
x <- runif(100)
t1 <- t2 <- NULL
for(i in 1:8){
t0_1 <- proc.time()[3]
aux <- mean(sample(x, n1[i], T))
tf_1 <- proc.time()[3]
t0_2 <- proc.time()[3]
aux <- mean(sample(x, n2[i], T))
tf_2 <- proc.time()[3]
t1[i] <- tf_1 - t0_1
t2[i] <- tf_2 - t0_2
}
t1
